<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="zinreloAPIKey">
                <display-name xml:lang="x-default">Zinrelo API Key</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableInCartRedemption">
                <display-name xml:lang="x-default">Enable In-cart Redemption</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableLoyalty">
                <display-name xml:lang="x-default">Enable Zinrelo</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableOrderConfirmationEvent">
                <display-name xml:lang="x-default">Enable Order Confirmation Event</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableOrderCreationEvent">
                <display-name xml:lang="x-default">Enable Order Creation Event</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableReturnOrCancellationEvent">
                <display-name xml:lang="x-default">Enable Order Return Or Cancellation Event</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloEnableRewardsOnPDP">
                <display-name xml:lang="x-default">Enable Reward Points Text On Product Pages</display-name>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloInCartDropdownText">
                <display-name xml:lang="x-default">In-cart Redemption Dropdown Text</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>You have {{AVAILABLE_POINTS}} points. Select your reward to redeem.</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloInCartRedemptionCancelText">
                <display-name xml:lang="x-default">In-cart Cancel Button Text</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloInCartRedemptionText">
                <display-name xml:lang="x-default">In-cart Redemption Text</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloPDPRewardsText">
                <display-name xml:lang="x-default">Rewards Points Text On Product Pages</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>Earn {{EARN_POINTS}} points by ordering this product.</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloPartnerID">
                <display-name xml:lang="x-default">Zinrelo Partner ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="zinreloWebhookURL">
                <display-name xml:lang="x-default">Zinrelo Webhook URL</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="ZinreloConfigs">
                <display-name xml:lang="x-default">Zinrelo Configurations</display-name>
                <attribute attribute-id="zinreloEnableLoyalty"/>
                <attribute attribute-id="zinreloPartnerID"/>
                <attribute attribute-id="zinreloAPIKey"/>
                <attribute attribute-id="zinreloWebhookURL"/>
                <attribute attribute-id="zinreloEnableInCartRedemption"/>
                <attribute attribute-id="zinreloInCartDropdownText"/>
                <attribute attribute-id="zinreloInCartRedemptionText"/>
                <attribute attribute-id="zinreloInCartRedemptionCancelText"/>
                <attribute attribute-id="zinreloEnableRewardsOnPDP"/>
                <attribute attribute-id="zinreloPDPRewardsText"/>
                <attribute attribute-id="zinreloEnableOrderCreationEvent"/>
                <attribute attribute-id="zinreloEnableOrderConfirmationEvent"/>
                <attribute attribute-id="zinreloEnableReturnOrCancellationEvent"/>
            </attribute-group>
        </group-definitions>
    </type-extension>
</metadata>
